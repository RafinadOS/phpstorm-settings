<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy Value">
        <Item>
          <IdeaActionID>XDebugger.CopyValue</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy Value</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'learning-cli.php'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌃D'</ShortCut>
          <Description>Debug 'learning-cli.php'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Force Run to Cursor">
        <Item>
          <IdeaActionID>ForceRunToCursor</IdeaActionID>
          <ShortCut>'⌥⌘F9'</ShortCut>
          <Description>Force Run to Cursor</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'bitrix24-cli.php'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'bitrix24-cli.php'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'learning-cli.php'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'learning-cli.php'</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run to Cursor">
        <Item>
          <IdeaActionID>RunToCursor</IdeaActionID>
          <ShortCut>'⌥F9'</ShortCut>
          <Description>Run to Cursor</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'scratch_36.php'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⇧F5'</ShortCut>
          <Description>Stop 'scratch_36.php'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>60</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update Project...">
        <Item>
          <IdeaActionID>Vcs.UpdateProject</IdeaActionID>
          <ShortCut>'⌘T'</ShortCut>
          <Description>Update Project...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="View Breakpoints...">
        <Item>
          <IdeaActionID>ViewBreakpoints</IdeaActionID>
          <ShortCut>'⇧⌘F8'</ShortCut>
          <Description>View Breakpoints...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'⌘[' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>